{
    "_version": "1.32.0",
    "sap.app": {
        "id": "ns.premises.premises",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/service/assets/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        }
    }, 
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.100.2",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "PremisesList",
                    "target": "PremisesList"
                },
                {
                    "pattern": "Premises({key}):?query:",
                    "name": "PremisesObjectPage",
                    "target": "PremisesObjectPage"
                },
                    {
                        "pattern": "Premises({key})/installations({key2}):?query:",
                        "name": "InstallationsObjectPage",
                        "target": "InstallationsObjectPage"
                    }
            ],
            "targets": {
                "PremisesList": {
                    "type": "Component",
                    "id": "PremisesList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Premises",
                            "variantManagement": "Page",
                            "navigation": {
                                "Premises": {
                                    "detail": {
                                        "route": "PremisesObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "PremisesObjectPage": {
                    "type": "Component",
                    "id": "PremisesObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "Premises",
                                "navigation": {
                                    "installations": {
                                        "detail": {
                                            "route": "InstallationsObjectPage"
                                        }
                                    }
                                }
                        }
                    }
                },
                    "InstallationsObjectPage": {
                        "type": "Component",
                        "id": "InstallationsObjectPage",
                        "name": "sap.fe.templates.ObjectPage",
                        "options": {
                            "settings": {
                                "editableHeaderContent": false,
                                "entitySet": "Installations"
                            }
                        }
                    }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}
